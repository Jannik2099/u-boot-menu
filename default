## /etc/default/u-boot - configuration file for u-boot-update(8)

U_BOOT_UPDATE="true"

U_BOOT_ALTERNATIVES="default recovery"
U_BOOT_DEFAULT="l0"
U_BOOT_ENTRIES="all"
U_BOOT_MENU_LABEL="Gentoo Linux"
U_BOOT_PARAMETERS="rw rootwait video=eDP-1:1920x1080@60"
U_BOOT_ROOT=""
U_BOOT_TIMEOUT="50"
U_BOOT_FDT="rockchip/rk3300-pinebook-pro.dtb"
U_BOOT_FDT_DIR="/boot/dtbs"			# Make sure this does NOT end with a /

